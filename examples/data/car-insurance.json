{
  "schema": {
    "type": "object",
    "properties": {
      "carNumber": {
        "type": "string",
        "required": true,
        "pattern": "\\w{2}\\d{5}"
      },
      "isNumberRegional": {
        "type": "boolean"
      },
      "importedAndNotClearedYet": {
        "type": "boolean",
        "required": true,
        "enum": [false]
      },
      "identNumber": {
        "type": "string",
        "required": true,
        "pattern": "\\d{6}"
      },
      "distance": {
        "type": "string",
        "required": true
      },
      "needHelpToMove": {
        "required": true,
        "type": "boolean"
      },
      "companyYouMoveFrom": {
        "required": true,
        "type": "string"
      },
      "policyNumber": {
        "required": true,
        "type": "string"
      },
      "insuranceRegisteredToYou": {
        "required": true,
        "type": "boolean"
      },
      "nameOfTheRegistrator": {
        "required": true,
        "type": "string"
      },
      "knowDateOfUpdate": {
        "type": "boolean"
      },
      "dateInOtherCompany": {
        "type": "string",
        "format": "date"
      },
      "bonusFromCompany": {
        "type": "string"
      },
      "bonusCarNumber": {
        "type": "string",
        "pattern": "\\w{2}\\d{5}"
      }
    }
  },


  "form": [
    {
      "type": "fieldset",
      "items": [
        {
          "key": "carNumber",
          "title": "Bilnummer",
          "type": "string",
          "placeholder": "Bilnummer"
        },
        {
          "key": "isNumberRegional",
          "type": "checkbox",
          "title": "Har ikke bilen reg.nr?"
        },
        {
          "type": "condition",
          "conditionalKey": "isNumberRegional",
          "conditionalValue": true,
          "items": [
            {
              "key": "importedAndNotClearedYet",
              "validationMessage": "Denne bilen kan dessverre ikke tegnes på nett, vennligst ta kontakt med kundeservice.",
              "title": "Importeres bilen fra utlandet og er ennå ikke fortollet i Norge ?",
              "type": "radios-inline",
              "titleMap": [
                {"value": true, "name": "Ja"},
                {"value": false, "name": "Nei"}
              ]

            }
          ]
        },
        {
          "type": "condition",
          "conditionalKey": "importedAndNotClearedYet",
          "conditionalValue": false,
          "items": [
            {
              "key": "identNumber",
              "type": "string",
              "title": "Understellsnummer – 6 siste tegn"
            }
          ]
        },
        {
          "key": "distance",
          "type": "string",
          "title": "Kilometerstand",
          "placeholder": "Kilometerstand"
        },
        {
          "key": "needHelpToMove",
          "title": "Vil du ha hjelp til oppsigelse",
          "type": "radios",
          "titleMap": [
            {"value": true, "name": "Ja, jeg flytter forsikring til Storebrand fra et annet selskap."},
            {"value": false, "name": "Nei, jeg har ingen forsikring som skal sies opp i annet selskap."}
          ]
        },
        {
          "type": "condition",
          "conditionalKey": "needHelpToMove",
          "conditionalValue": true,
          "items": [
            {
              "key": "companyYouMoveFrom",
              "type": "select",
              "title": "Angi selskap du flytter fra.",
              "titleMap": {
                "AIG EUROPE": "AIG EUROPE",
                "CODAN": "CODAN"
              }
            },
            {
              "key": "policyNumber",
              "type": "string",
              "title": "Polisenummer i eksisterende selskap"
            },
            {
              "key": "insuranceRegisteredToYou",
              "type": "radios",
              "title": "Er forsikringen i nåværende selskap registrert i ditt navn?",
              "titleMap": [
                {"value": true, "name": "Ja"},
                {"value": false, "name": "Nei"}
              ]
            },
            {
              "type": "condition",
              "conditionalKey": "insuranceRegisteredToYou",
              "conditionalValue": false,
              "items": [
                {
                  "key": "nameOfTheRegistrator",
                  "type": "string",
                  "title": "Navn og fødselsdato på forsikringstaker i nåværende selskap"
                }
              ]
            },
            {
              "key": "knowDateOfUpdate",
              "type": "radios",
              "title": "Vet du dato for hovedforfall for denne forsikringen i ditt nåværende selskap?",
              "titleMap": [
                {"value": true, "name": "Ja"},
                {"value": false, "name": "Nei"}
              ]
            },
            {
              "type": "condition",
              "conditionalKey": "knowDateOfUpdate",
              "conditionalValue": true,
              "items": [
                {
                  "key": "dateInOtherCompany",
                  "title": "Dato for forfall i annet forsikringsselskap"
                }
              ]
            }
          ]
        },
        {
          "type": "condition",
          "conditionalKey": "needHelpToMove",
          "conditionalValue": false,
          "items": [
            {
              "key": "bonusFromCompany",
              "type": "select",
              "title": "Angi selskap vi skal hente bonus fra.",
              "titleMap": {
                "AIG EUROPE": "AIG EUROPE",
                "CODAN": "CODAN"
              }
            },
            {
              "key": "bonusCarNumber",
              "title": "Bilnummer",
              "type": "string",
              "placeholder": "Bilnummer"
            }
          ]
        },
        {
          "type": "submit",
          "title": "finish",
          "style": "stb-btn"
        }
      ]
    }
  ]
}
