        {
          "key": "streetAddress",
          "type": "string",
          "title": "Gateadresse",
          "validationMessage": "Vennligst oppgi gateadresse",
          "dependencies": ["_addressType"],
          "expression": "_addressType === 'useStreetAddress'"
