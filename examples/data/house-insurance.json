{
  "schema": {
    "type": "object",
    "properties": {
      "_addressType": {
        "type": "string"
      },
      "streetAddress": {
        "required": true,
        "type": "string"
      },
      "cadastralGnr": {
        "required": true,
        "type": "string"
      },
      "cadastralBnr": {
        "required": true,
        "type": "string"
      },
      "intendsToMigrateInsurance": {
        "required": true,
        "type": "boolean"
      },
      "oldInsuranceCompany": {
        "required": true,
        "type": "object"
      },
      "customerPreviousInsurancePolicyholder": {
        "required": true,
        "type": "boolean"
      },
      "previousInsurancePolicyholderNameSSN": {
        "required": true,
        "type": "string"
      },
      "expirationDateKnown": {
        "type": "boolean"
      },
      "_expirationDateDay": {
        "required": true,
        "type": "string",
        "format": "date-format"
      },
      "expirationDateDay": {
        "required": true,
        "type": "string",
        "format": "date-format"
      },
      "issueDay": {
        "required": true,
        "type": "string",
        "format": "date-format"
      }
    }
  },


