        {
          "key": "_addressType",
          "title": "Adresse",
          "validationMessage": "Vennligst gjør et valg",
          "infoMessage": "todo",
          "type": "radios",
          "titleMap": [
            {"value": "useStreetAddress", "name": "Gateadresse"},
            {"value": "useCadastralAddress", "name": "Uten gateadresse"}
          ],
          "dependencies": ["houseContAdrSameAsHomeAdr"],
          "expression": "houseContAdrSameAsHomeAdr === false"
